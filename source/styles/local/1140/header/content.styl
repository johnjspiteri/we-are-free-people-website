#header
	height 76px
	padding 8px 0 0 0

	&__container
		width 1024px

	&__brand
		width 316px

		h1
			margin 10px 0 0 0
			font-size 15px
			line-height 15px

		h2
			font-size 25px
			line-height 25px

	&__logo
		width 64px
		height 60px
		margin 10px 0 0 0

	&__two
	&__three
		width 26px
		height 42px
		margin 18px 0 0 25px

		svg
			width 52px
			margin 0 0 0 -13px

	&__navigation
		width: 100%

		&__container
			margin 0 auto
			width 1024px

.header-leaf
	width 35px

	svg
		width 62px
		margin 0 0 0 -14px

.header
	&__tile
		height 49px
		padding 0 34px

		h3
			font-size 12px
			line-height 50px

	&__tile.active
		height 50px

		h3
			line-height 51px

	&__tile.end.active
		border-right $secondary-border

		h3
			color $paragraph

	&__tile.end
		border-right $secondary-border
