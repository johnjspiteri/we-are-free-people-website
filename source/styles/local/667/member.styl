.team-list
	width 300px
	margin 0 0 30px 10px

	&__square
		width 280px
		padding 16px 5px 18px 15px
		margin-bottom: 14px

	&__left
		width 96px
		margin 0 15px 4px 0

		h2
			height 28px
			padding 0 11px 0 11px
			margin 0 5px 0 0
			font-size 18px
			line-height 29px

	&__image
		width 96px
		height 96px
		margin-bottom: 10px

	&__content
		margin-bottom: 16px

		h1
			margin 0 4px 8px 0
			font-size 24px
			line-height 27px

	&__name
		margin-bottom: 14px

		h2
			font-size 18px
			line-height 25px

	&__description
		width 270px
		margin-bottom: 18px

		p
			font-size 18px
			line-height 25px

	&__contact
		width 34px
		height 34px
		margin 0 8px 0 0

		&__linkedin-in
			font-size 18px
			line-height 34px

		&__mobile-alt
			font-size 19px
			line-height: 35px

		&__at
			font-size 16px
			line-height: 36px
