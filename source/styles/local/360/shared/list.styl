.list-item
	float left
	margin: 0;
	padding 15px 10px 15px 10px
	background $secondary-background-color
	border $primary-on-base-border
	border-radius $radius
	box-sizing border-box
	cursor pointer

	h1
		float left
		width: 100%
		margin 0 0 18px 0
		padding 0 0 5px
		font-family $primary-font
		font-weight $primary-font-weight-400
		border-bottom $secondary-title-border-bottom
		font-size 17px
		line-height 23px
		color $black-color
		text-align: center

	h4
		float: left
		width: 100%
		margin 0 0 7px 0
		padding 0
		font-family $primary-font
		font-weight $primary-font-weight-400
		font-size 13px
		line-height 13px
		color $primary-button-text-active-color
		text-align center
		box-sizing border-box

	p
		float left
		width 100%
		margin 0 0 28px 0
		padding 0
		font-family $secondary-font
		font-weight $secondary-font-weight-400
		font-size 13px
		line-height 20px
		color $secondary-paragraph-color
		text-align: center;
		box-sizing border-box

	&__content
		float left
		width 100%
		margin 0
		padding 0 10px
		box-sizing border-box

	&__detail
		float left
		width 100%
		margin 0
		padding 10px
		background $primary-background-color
		border $primary-on-secondary-border
		border-radius $radius
		text-align center
		box-sizing border-box

	&__half
		float left
		width 50%
		padding 0 5px
		margin: 0 0 10px 0
		box-sizing border-box

		h2
			float left
			width 100%
			margin 0
			padding 10px 0
			font-family $secondary-font
			font-size 14px
			font-weight $secondary-font-weight-400
			line-height 14px
			color $primary-title-color
			background $primary-button-inactive-background-color
			border $primary-button-inactive-border
			border-radius $radius
			text-align center
			box-sizing border-box

	&__tags
		float left
		width 100%
		margin 0
		padding 0

		h3
			display inline-block
			margin 0 5px
			padding 7px 14px
			font-family $secondary-font
			font-weight $secondary-font-weight-300
			border $base-on-primary-border
			border-radius $radius
			font-size 11px
			line-height: 14px
			background $base-background-color
			color $primary-title-color
			text-align center

	&__holder
		float left
		width 100%
		margin 0 0 15px 0
		padding 0
		box-sizing border-box

	&__career
		float left
		width 100%
		margin 0 0 8px 0
		padding 0 5px
		box-sizing border-box

	&__team
		float left
		margin 0 10px 0 0
		padding: 0

		h2
			display flex
			height: 28px
			margin 0
			padding 0 14px
			font-family $primary-font
			font-weight $primary-font-weight-400
			border-radius $radius
			border $primary-on-base-border
			font-size 10px
			line-height 14px
			color $primary-title-color
			background $primary-background-color
			text-align left
			box-sizing border-box
			align-items center
			justify-self center
			justify-content center

		h3
			margin 0 0 5px
			padding 0
			font-family $primary-font
			font-weight $primary-font-weight-400
			font-size 10px
			line-height 10px
			text-align center
			color $grey-color
			box-sizing border-box

	&__contact
		float left
		width 100%
		margin 0
		padding 15px 10px
		border $primary-button-active-border
		border-radius $radius
		background $primary-background-color
		text-align center
		box-sizing border-box

	&__square
		float left
		margin 0 30px 15px 0
		padding: 0

		h4
			display block
			margin 0 0 10px 0
			padding 0
			font-family $primary-font
			font-weight $primary-font-weight-400
			font-size 11px
			line-height 12px
			color $primary-button-text-active-color
			text-align left
			box-sizing border-box

		h5
			display block
			margin 0 0 5px 0
			padding 0
			font-family $primary-font
			font-weight $primary-font-weight-400
			font-size 13px
			line-height 13px
			color $primary-button-text-active-color
			text-align left
			text-transform: uppercase

		img
			position relative
			width 50px
			height 50px
			margin: 0 auto
			padding 0
			border $black-border
			border-radius $radius-circle
			overflow hidden

	&__square:nth-child(1)
		margin 0 30px 10px 10px

	&__buttons
		float left
		width 100%
		margin: 0
		padding: 0
		box-sizing border-box

	&__button
		float left
		width 50%
		height: 30px
		margin 0
		padding 0 10px
		box-sizing border-box

		h6
			display flex
			height 32px
			border-radius $radius
			background $primary-accent-background-color
			margin 0
			padding 0
			font-family $primary-font
			font-weight $primary-font-weight-400
			font-size 13px
			color $grey-color
			align-self center
			justify-content center
			box-sizing border-box
			align-items center


