	&__full
		float left
		width 280px
		margin 0
		padding 0

.list-button
	float left
	width 145px
	height 30px
	margin 0 10px 0 0
	padding 0
	border-radius $radius
	border $primary-on-base-border
	box-sizing border-box
	background $primary-button-background-active-color

	h2
		display flex
		width 100%
		height 100%
		margin 0
		padding 0
		font-family $primary-font
		font-weight $primary-font-weight-400
		font-size 13px
		line-height: 13px
		color $color-base-on-primary
		align-items center
		justify-content center

.list-button:last-child
	margin 0
