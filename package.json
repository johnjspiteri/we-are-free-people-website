	},
	"eslintConfig": {
		"extends": [
			"@interface-agency/eslint-config-angular",
			"prettier"
		],
		"ignorePatterns": [
			"node_modules"
		],
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "error"
		}
	},
	"name": "interface-agency-website",
	"prettier": "@interface-agency/prettier-style",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org/interface-agency-website/website/src/master/"
	},
	"scripts": {
		"analysis:d": "npm run rimraf -- \"development\" && cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" webpack --config build/development/configuration.ts --analyze",
		"analysis:p": "npm run rimraf -- \"production\" && cross-env TS_NODE_PROJECT=\"build/production/tsconfig.json\" webpack --config build/production/configuration.ts --analyze",
		"build:d": "npm run rimraf -- \"source/dll\" && cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" webpack --config build/dll/configuration.ts",
		"development": "cross-env TS_NODE_PROJECT=\"./build/tsconfig.json\" webpack serve --config ./build/development/configuration.ts --color",
		"fix": "eslint source --ext .ts --fix",
		"lint": "eslint source --ext .ts",
		"production": "npm run rimraf -- \"production\" && cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" webpack --config build/production/configuration.ts --color",
		"rimraf": "rimraf",
		"serve": "cd ./production && http-server"
	},
	"version": "1.0.0"
}
